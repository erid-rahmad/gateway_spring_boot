<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE j8583-config PUBLIC "-//J8583//DTD CONFIG 1.0//EN"
        "http://j8583.sourceforge.net/j8583.dtd">
<j8583-config>

    <!-- echo request -->
    <template type="0800">
        <field num="70" type="NUMERIC" length="3">301</field>
    </template>

    <!-- The server example uses this to read the requests -->
    <parse type="0200">
        <field num="2" type="NUMERIC" length="16"/>
        <field num="3" type="NUMERIC" length="6"/>
        <field num="7" type="DATE10"/>
        <field num="11" type="NUMERIC" length="6"/>
        <field num="12" type="TIME"/>
        <field num="13" type="DATE4"/>
        <field num="18" type="NUMERIC" length="4"/>
        <field num="32" type="LLVAR"/>
        <field num="33" type="LLVAR"/>
        <field num="37" type="NUMERIC" length="12"/>
        <field num="41" type="NUMERIC" length="8"/>
        <field num="49" type="ALPHA" length="3"/>
        <field num="120" type="LLLVAR">
            <field num="1" type="NUMERIC" length="4"/>
            <field num="2" type="ALPHA" length="50"/>
            <field num="3" type="ALPHA" length="100"/>
            <field num="4" type="ALPHA" length="50"/>
            <field num="5" type="NUMERIC" length="11"/>
            <field num="6" type="ALPHA" length="20"/>
        </field>
        <field num="121" type="LLLVAR">
            <field num="1" type="ALPHA" length="255"/>
            <field num="2" type="ALPHA" length="255"/>
            <field num="3" type="ALPHA" length="255"/>
        </field>
        <field num="120" type="LLLVAR"/>
    </parse>

    <parse type="0210" extends="0200">
        <field num="39" type="ALPHA" length="2"/>
        <field num="121" type="LLLVAR">
            <field num="1" type="ALPHA" length="255"/>
            <field num="2" type="ALPHA" length="255"/>
            <field num="3" type="ALPHA" length="255"/>
            <field num="4" type="ALPHA" length="1"/>
        </field>
    </parse>

    <parse type="0800">
        <field num="7" type="DATE10"/>
        <field num="11" type="NUMERIC" length="6"/>
        <field num="70" type="ALPHA" length="3"/>
    </parse>

    <parse type="0810" extends="0800">
        <field num="39" type="ALPHA" length="2"/>
    </parse>

</j8583-config>
